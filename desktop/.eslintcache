 []);\n\n\n  // console.log(task.subtask.filter(item => item.id !== 27));\n\n  // console.log(setTask(task.subtask.some(item => item.id !== 27)))\n  const contextMenu = () => {\n    window.addEventListener('contextmenu', (e) => {\n       const menu = new Menu();\n       if (e.target.dataset.subtask) {\n         menu.append(new MenuItem({\n           label: \"Delete Subtask\",\n           click: () => {\n             _deleteSubTask(e.target.dataset.id)\n           }\n         }));\n       }\n       if (e.target.dataset.message) {\n         menu.append(new MenuItem({\n           label: \"Create Subtask from Selection\",\n           click: () => {\n             _deleteSubTask('000')\n           }\n         }));\n         menu.append(new MenuItem({\n           type: \"separator\",\n         }));\n         menu.append(new MenuItem({\n           label: \"Delete Message\",\n           click: () => {\n             _deleteSubTask('000')\n           }\n         }));\n       }\n       menu.popup({ window: window.remote.getCurrentWindow() })\n     }, false)\n  }\n\n\n  return (\n    <>\n\n    {loading\n      ? <p></p>\n      : (\n        <>\n\n      <Header\n        back={app.setTask}\n      />\n\n      <article className=\"main container\">\n\n      <ul>\n        <li\n        >\n          <Textarea\n            data={task.name}\n          />\n        </li>\n      </ul>\n\n\n      { task.subtask.length > 0 &&\n        <ul>\n        <span className=\"label pb2\">Sub Tasks</span>\n          {task.subtask.map((item, index) =>\n            <li>\n              <CardSubTask\n                key={index}\n                data={item}\n              />\n            </li>\n          )}\n        </ul>\n      }\n\n\n        <ul>\n          <span className=\"label pb2\">Description</span>\n          <li\n          >\n            <Textarea\n              data={task.description}\n            />\n          </li>\n        </ul>\n\n\n        {task.message.length > 0 &&\n        <ul>\n          <span className=\"label pb2\">Messages</span>\n\n          {task.message.map((item, index) =>\n            <li>\n              <CardMessage\n                key={index}\n                data={item}\n              />\n            </li>\n          )}\n\n        </ul>\n        }\n\n\n      </article>\n\n      <FooterModal\n        setModalState={setModalState}\n        modalState={modalState}\n      >\n          <a\n            className=\"btn btn--secondary\"\n            onClick={() => _createSubTask()}\n          >New Sub Task</a>\n      </FooterModal>\n\n      <Footer>\n        <a\n          className=\"btn btn--none\"\n          onClick={() => setModalState(!modalState)}>\n        AD</a>\n      </Footer>\n\n      </>\n    )}\n      </>\n    )\n  }\n\nexport default Task\n","/Users/think/Arbeit/development/projects/cxapp/desktop/src/context/AppContext.js",[],"/Users/think/Arbeit/development/projects/cxapp/desktop/src/components/CardProject.js",["219","220"],"import react from \"react\";\n\nconst CardProject = ( { data }) => {\n\n  return (\n\n    <>\n      <a data-project data-id={data.id}>\n        <div className=\"pr3\">IC</div>\n        <div>\n        <h2 className=\"pb1\">{data.name}</h2>\n        {(data.client !== null && data.client !== undefined)\n          ? <span className=\"info\">{data.client.name}</span>\n          : <span className=\"info\">Personal</span>\n        }\n        </div>\n      </a>\n    </>\n\n  )\n}\n\nexport default CardProject\n","/Users/think/Arbeit/development/projects/cxapp/desktop/src/components/FooterModal.js",["221","222","223"],"import react, { useRef, useState, useEffect } from \"react\";\n\nconst FooterModal = ( { children, modalState, setModalState }) => {\n\n  const node = useRef();\n\n  const handleClick = e => {\n    if (node.current.contains(e.target)) {\n      return;\n    }\n    setModalState(false);\n  };\n\n  useEffect(() => {\n    document.addEventListener(\"mousedown\", handleClick);\n\n    return () => {\n      docume